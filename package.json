{
  "name": "travelio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "npm run db:push && npm run start",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "check:node": "check-node-version --node 18 --npm 8",
    "preinstall": "npm run check:node",
    "postbuild": "next-sitemap",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate": "npx prisma migrate dev --name <MIGRATION_NAME>",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:reset": "prisma db reset",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "@prisma/client": "^4.6.1",
    "@vercel/analytics": "^0.1.5",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "check-node-version": "^4.2.1",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "cookies": "^0.8.0",
    "date-fns": "^2.29.3",
    "easy-peasy": "^5.1.0",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.9",
    "gen-env-types": "^1.3.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "13.0.4",
    "next-sitemap": "3.1.32",
    "next-transpile-modules": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-select": "^5.6.1",
    "rss": "^1.2.2",
    "sass": "^1.56.1",
    "sharp": "^0.31.2",
    "string-hash": "^1.1.3",
    "swr": "^1.3.0",
    "tinycolor2": "^1.4.2",
    "webpack-bundle-analyzer": "^4.7.0",
    "yup": "^0.32.11",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-howler": "^5.2.0",
    "@types/rss": "^0.0.29",
    "@types/string-hash": "^1.1.1",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-plugin-superjson-next": "^0.4.4",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "jest-resolve": "^29.3.1",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "13.0.3",
    "prettier": "^2.8.0",
    "prisma": "^4.6.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vercel": "28.5.6"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  }
}
